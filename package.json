	"main": "./build/index.js",
	"man": "./readme.md",
	"scripts": {
		"build": "npm run compile",
		"deploy": "npm run build && npm run test && npm login && npm publish",
		"deleteBuild": "rm -rf ./build && rm .tsbuildinfo",
		"pretty": "node ./node_modules/prettier/bin-prettier.js --write . --use-tabs --cache --cache-strategy metadata --ignore-unknown --loglevel=warn",
		"compile": "tsc",
		"start": "node ./build/index.js",
		"runTest": "for file in ./build/testing/*.js; do echo $file && time node $file || exit 1; done;",
		"runFile": "time node",
		"test": "npm run build && npm run runTest",
		"testOne": "npm run build && npm run runFile"
	},
